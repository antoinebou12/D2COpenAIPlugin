{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/generate_mermaid",
      "dest": "/app.py/generate_mermaid"
    },
    {
      "src": "/generate_diagram/(.*)",
      "dest": "/app.py/generate_diagram"
    },
    {
      "src": "/openapi.yaml",
      "dest": "/app.py/openapi.yaml"
    },
    {
      "src": "/.well-known/ai-plugin.json",
      "dest": "/app.py/.well-known/ai-plugin.json"
    },
    {
      "src": "/logo.png",
      "dest": "/app.py/logo.png"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ],
  "static": {
    "source": "static",
    "basePath": "/static"
  },
  "public": {
    "source": "public",
    "basePath": "/public"
  }
}
